{
    "name": "verbb/default-dashboard",
    "description": "Set default dashboard widgets for users.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/default-dashboard/issues?state=open",
        "source": "https://github.com/verbb/default-dashboard",
        "docs": "https://github.com/verbb/default-dashboard",
        "rss": "https://github.com/verbb/default-dashboard/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "verbb/base": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
          "verbb\\defaultdashboard\\": "src/"
        }
    },
    "extra": {
        "name": "Default Dashboard",
        "handle": "default-dashboard",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/default-dashboard/craft-4/CHANGELOG.md",
        "class": "verbb\\defaultdashboard\\DefaultDashboard"
    }
